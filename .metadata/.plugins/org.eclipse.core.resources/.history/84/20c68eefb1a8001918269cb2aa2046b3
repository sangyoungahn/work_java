package com.ssafy.java;

public class AlpaTest2 {

	public static void main(String[] args) {
		int alpa = 65;

		for(int i = 0; i < 5; i++) {
			for(int j = 4-i; j >= 0; j--) {
				System.out.print(" ");
			}
			System.out.println();
			for(int j = i ; j < 0; i++) {
				System.out.print((char)alpa);
				alpa++;
			}
		}
	}

}